{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "allowJs": false,
        "noEmitOnError": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}